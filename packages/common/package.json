{
  "name": "@eliza-dot-aes/common",
  "version": "1.0.7",
  "description": "Common interfaces and utilities for Eliza-dot-AES",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "!src/**/*.spec.ts"
  ],
  "scripts": {
    "build": "tsc",
