  "keywords": ["crypto", "aes", "sr25519", "encryption"],
  "author": "weimeme",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChainSupport/eliza-dot-aes.git"
  },
  "dependencies": {
    "@eliza-dot-aes/common": "^1.0.0",
    "@scure/sr25519": "^0.3.0"
  },
  "devDependencies": {
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  }
}